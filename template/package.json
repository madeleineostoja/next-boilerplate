{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
<% if (features.sitemap) { -%>
      "postbuild": "next-sitemap",
<% } -%>
    "build:storybook": "build-storybook -o ./storybook -s ./public --quiet --docs",
    "clean": "rm -rf ./dist ./storybook",
    "develop": "next dev",
    "develop:storybook": "start-storybook -p 3002 -s ./public --ci --quiet",
    "develop:all": "concurrently npm:develop npm:develop:storybook",
    "generate": "hygen new",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
<% if (features.firestore) { -%>
    "@nandorojo/swr-firestore": "^0.16.0",
<% } -%>
    "@svgr/webpack": "^5.5.0",
    "array-flat-polyfill": "^1.0.1",
    "case": "^1.6.3",
    "clean-deep": "^3.4.0",
    "next": "^10.0.6",
    "next-compose-plugins": "^2.2.1",
    "next-fonts": "^1.5.1",
    "next-pwa": "^5.0.5",
    "next-seo": "^4.19.0",
<% if (features.sitemap) { -%>
    "next-sitemap": "^1.4.17",
<% } -%>
    "pollen-css": "^1.1.0",
<% if (features.prisimc) { -%>
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^3.0.2",
<% } -%>
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
<% if (features.prisimc) { -%>
    "react-html-renderer": "^0.3.2",
<% } -%>
    "react-responsive": "^8.2.0",
<% if (features.formComponents) { -%>
    "react-select": "^4.0.2",
    "react-textarea-autosize": "^8.3.0",
<% } -%>
    "react-use": "^17.1.0",
    "satchel-css": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@next/bundle-analyzer": "^10.0.6",
    "@storybook/addon-docs": "^6.1.16",
    "@storybook/addons": "^6.1.16",
    "@storybook/react": "^6.1.16",
    "@types/node": "^14.14.22",
<% if (features.prisimc) { -%>
    "@types/prismic-dom": "^2.1.1",
<% } -%>
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
<% if (features.formComponents) { -%>
    "@types/react-select": "^4.0.12",
<% } -%>
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "hygen": "^6.0.4",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1"
  }
}
